{
    "builds": [
        {
            "src": "Forecasting_API/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "python-version": "3.11.1",
                "maxLambdaSize": "500mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "Forecasting_API/wsgi.py"
        }
    ]
}